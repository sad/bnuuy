{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "arrow-body-style": "off",
        "no-use-before-define": "off",
        "consistent-return": "off"
    }
}
